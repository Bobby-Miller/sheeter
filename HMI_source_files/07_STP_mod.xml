                <![CDATA[Private Sub Button3_Released()

Dim TimeStamp As Date
Dim cQuality As tagQualityConstants
Dim cSubStatus As tagSubStatusConstants
Dim cLimit As tagLimitConstants
Dim s1 As String
Dim file_name As String
Dim fnum As Integer
Dim File_Content As String
Dim Temp_row As Variant
Dim rows As Variant
Dim num_cols As Long
Dim num_rows As Long
Dim Arreglo() As String
Dim R As Long
Dim C As Long
Dim row_indx As Integer
Dim ArrayTag(0 To 30, 0 To 10) As Tag
Dim RecIndex As Tag
Dim LoadStatus As Tag
Dim PartNo As Tag
          
Button2.Enabled = False
Me.CommonDialog.Filter = "CSV File (*.csv)|*.CSV|All files (*.*)|*.*"
Me.CommonDialog.DialogTitle = "Select File"
Me.CommonDialog.CancelError = True
On Error GoTo errhandler

Me.CommonDialog.ShowOpen

otag.Value = Me.CommonDialog.FileName

    otag.GetTagData vValue, TimeStamp, cQuality, cSubStatus, cLimit
    otag.RefreshFromSource
    file_name = otag.Value 'vValue
    otag.Value = Right(file_name, 50)
    fnum = FreeFile
    Open file_name For Input As fnum
    File_Content = Input$(LOF(fnum), #fnum)
    Close fnum
    
    rows = Split(File_Content, vbCrLf)
    num_rows = UBound(rows) - 1
    Temp_row = Split(rows(0), ",")
    num_cols = UBound(Temp_row)
    ReDim Arreglo(num_rows, num_cols)

    'Mem reserve for file loading in to PLC registers
            oGroup.Add "{SYS_NUMBER_OF_PLYS}"
            oGroup.Add "{FileLoad\LoadProgress}"
            oGroup.Add "{PART_NUMBER}"
    
    For row_indx = 0 To num_rows - 5
        oGroup.Add ("{FileLoad\Material_" + Format(row_indx, "#0") + "}")
        oGroup.Add ("{FileLoad\Stop_" + Format(row_indx, "#0") + "}")
    Next row_indx
    
    'Carga del archivo a los registros del PLC
    
    Set RecIndex = oGroup.Item("{SYS_NUMBER_OF_PLYS}")
    Set LoadStatus = oGroup.Item("{FileLoad\LoadProgress}")
    Set PartNo = oGroup.Item("{PART_NUMBER}")
    
    For row_indx = 0 To num_rows - 5
        Set ArrayTag(row_indx, 2) = oGroup.Item("{FileLoad\Material_" + Format(row_indx, "#0") + "}")
        Set ArrayTag(row_indx, 4) = oGroup.Item("{FileLoad\Stop_" + Format(row_indx, "#0") + "}")
    Next row_indx
    
    'MsgBox "set array", vbOKOnly, "Valor"
    
    Temp_row = Split(rows(0), ",")
    PartNo.Value = Temp_row(1)

    For R = 5 To num_rows
        Temp_row = Split(rows(R), ",")
        Arreglo(R, 2) = Temp_row(2)
        Arreglo(R, 4) = Temp_row(4)
        
        ArrayTag(R - 5, 2).Value = Arreglo(R, 2)
        ArrayTag(R - 5, 4).Value = Arreglo(R, 4)
    Next R
    
    'PLC mesages Number of records loaded and Status = 2 = recipe loaded successful
   
    RecIndex.Value = num_rows - 4
    LoadStatus.Value = 2
    
    MsgBox "File Loaded", vbOKOnly, "Message"
    
GoTo salir

errhandler:
    If Err = cdlCancel Then
        MsgBox "Operation canceled, No file loaded", vbOKOnly, "Message"
    Else
        If Err.Number <> 0 Then
        Msg = "Error # " & Str(Err.Number) & " was generated by " & Err.Source & Chr(13) & Err.Description
        MsgBox Msg, vbOKOnly, "Error"
        End If
    End If
    
    'If Err.Number <> 0 Then
   ' Msg = "Error # " & Str(Err.Number) & " was generated by " & Err.Source & Chr(13) & Err.Description
    'MsgBox Msg, vbOKOnly, "Error"
    'End If

salir:

    Button3.Enabled = True

End Sub
